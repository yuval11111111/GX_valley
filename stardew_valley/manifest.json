{
  "manifest_version": 3,
  "name": "Stardew_valley",
  "description": "A Stardew valley mod by YuvalTheCookie",
  "version": "1.0",
  "icons": {
    "512": "icon-cookie-1.png"
  },
  "developer": {
    "name": "YuvalTheCookie"
  },
  "mod": {
    "license": "license.txt",
    "schema_version": 1,
    "payload": {
      "wallpaper": {
        "dark": {
          "image": "wallpaper/stardew-valley.jpg",
          "text_color": "#f5f5f5",
          "text_shadow": "#010101"
        },
        "light": {
          "image": "wallpaper/stardew-valley.jpg",
          "text_color": "#f5f5f5",
          "text_shadow": "#010101"
        }
      },
      "background_music": ["music/menu.mp3"],
      "page_styles": [
        {
          "matches": ["https://*.discord.com/*"],
          "css": ["css/discord.main.theme.css", "css/discord.changecolors.css"]
        }
      ]
    }
  }
}
